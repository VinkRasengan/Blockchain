{
  "name": "mycoin",
  "version": "1.0.0",
  "description": "MyCoin - A decentralized cryptocurrency web wallet with blockchain implementation",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "concurrently \"tsc -w\" \"wait-on dist/server.js && nodemon dist/server.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "serve": "npm run build && node dist/server.js",
    "serve:prod": "NODE_ENV=production npm run build && node dist/server.js"
  },
  "keywords": [
    "blockchain",
    "cryptocurrency",
    "wallet",
    "web",
    "decentralized",
    "bitcoin",
    "ethereum",
    "proof-of-work",
    "proof-of-stake"
  ],
  "author": "MyCoin Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/ws": "^8.5.0",
    "@types/elliptic": "^6.4.0",
    "@types/qrcode": "^1.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "wait-on": "^7.0.0",
    "nodemon": "^3.0.0"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "elliptic": "^6.5.4",
    "level": "^8.0.0",
    "ws": "^8.14.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^7.0.0",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "qrcode": "^1.5.3",
    "bip39": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VinkRasengan/Blockchain.git"
  },
  "bugs": {
    "url": "https://github.com/VinkRasengan/Blockchain/issues"
  },
  "homepage": "https://github.com/VinkRasengan/Blockchain#readme"
}
